{
  "version": 2,
  "functions": {
    "app.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/public/auth/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://snappy-git.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
